<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ameron32.tap.conventionnotes"
    android:versionCode="0113"
    android:versionName="1.00" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="19" />


    
    <uses-permission android:name="android.permission.INTERNET"/>
    
    <application
        android:name="com.ameron32.tap.conventionnotes.CustomApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >

        <activity
            android:name="com.ameron32.tap.conventionnotes.MainActivity"
            android:label="@string/app_name"
            android:launchMode="singleInstance" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.ameron32.tap.conventionnotes.SettingsActivity"
            android:label="@string/title_activity_settings"
            android:parentActivityName="com.ameron32.tap.conventionnotes.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.ameron32.tap.conventionnotes.MainActivity" />
        </activity>

    
    
    
    <meta-data android:name="com.crashlytics.ApiKey" android:value="451a4e784d66f9e46edd52c2e4fe58e0ba5f1f4c"/>
    </application>

</manifest>
